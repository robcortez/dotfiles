from jinja2 import Markup
from datetime import datetime

class date_organizer:
    def __init__(self, datelist):
        self.datelist = datelist

    def render(self):
        prev_date = datetime.utcnow()
        for d in datelist:
            if d > prev_date:
                prev_date = d
                return True
            else:
                prev date = d
                return False
